/* pages/category/category.wxss */
.category {
  margin: 10px;
  background: #fff;
  padding: 10px 16px;
}
.category .tab-change .output,.category .tab-change .input {
  display: inline-block;
  height: 30px;
  line-height: 30px;
  width: 60px;
  border: 1px solid #D7D7D7;
  text-align: center;
}
.category .tab-change .output {
  border-right: 0px;
}
.category .tab-change .input {
  border-left: 0px;
}
.category .tab-change .active-tab-output {
  background: #FF2712;
  color: #fff;
  border-color: #FF2712;
}
.category .tab-change .active-tab-input {
  background: #66B032;
  color: #fff;
  border-color: #66B032;
}
.van-swipe-cell__right {
  height: 100%;
  line-height: 2.5;
  width: 64px;
  text-align: center;
  background: red;
  color: #fff;
}
.fa {
  position: relative;
  top: 6px;
  margin-right: 10px;
}
.bottom-btn {
  text-align: center;
  margin-top: 20px;
  padding: 10px 0 20px 0;
}